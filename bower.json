{
  "name": "BlueNumber",
  "private": "true",
  "devDependencies": {
    "ionic": "driftyco/ionic-bower#1.3.1",
    "ngCordova": "^0.1.27-alpha"
  },
  "dependencies": {
    "angular-translate": "~2.11.1",
    "angular-translate-loader-static-files": "^2.11.1",
    "angular-base64": "^2.0.5",
    "ngstorage": "^0.3.11",
    "ui-leaflet": "^1.0.1",
    "leaflet.markercluster": "^0.5.0",
    "font-awesome": "fontawesome#^4.6.3",
    "ion-autocomplete": "^0.3.2"
  },
  "resolutions": {
    "angular": ">=1.4.x"
  },
  "install": {
    "base": "www/lib",
    "path": {
      "scss": "{name}/scss",
      "css": "{name}/css",
      "js": "{name}/js",
      "eot": "{name}/fonts",
      "otf": "{name}/fonts",
      "ttf": "{name}/fonts",
      "woff": "{name}/fonts",
      "woff2": "{name}/fonts",
      "svg": "{name}/fonts"
    },
    "ignore": [
      "angular",
      "angular-animate",
      "angular-ui-router"
    ],
    "sources": {
      "ionic": [
        "bower_components/ionic/js/ionic.bundle.js",
        "bower_components/ionic/scss/**",
        "bower_components/ionic/fonts/**"
      ],
      "font-awesome": [
        "bower_components/font-awesome/scss/**",
        "bower_components/font-awesome/fonts/**"
      ]
    }
  }
}
